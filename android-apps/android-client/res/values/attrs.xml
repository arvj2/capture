<?xml version="1.0" encoding="utf-8"?>
<resources>
        
    <attr name="typeface" format="enum">
        <enum name="robotoBold" value="1"/>
        <enum name="robotoLight" value="2"/>
        <enum name="robotoMedium" value="3"/>
        <enum name="robotoRegular" value="4"/>
        <enum name="robotoThin" value="5"/>
        <enum name="entypo" value="6"/>
    </attr>
    
    <declare-styleable name="CustomTypeface">
        <attr name="typeface"/>
    </declare-styleable>

    <declare-styleable name="MaskedText">
        <attr name="mask" format="string|reference"/>
    </declare-styleable>
    
</resources>
